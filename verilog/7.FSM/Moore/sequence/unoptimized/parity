S_0x558fc9521d00 .scope module, "test_parity" "test_parity" 2 1;
 .timescale 0 0;
v0x558fc9538250_0 .var "clk", 0 0;
v0x558fc9538320_0 .var "x", 0 0;
v0x558fc95383f0_0 .net "z", 0 0, v0x558fc9538110_0;  1 drivers
S_0x558fc9521e90 .scope module, "PAR" "parity_gen" 2 4, 3 1 0, S_0x558fc9521d00;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "x";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /OUTPUT 1 "z";
P_0x558fc950c7d0 .param/l "EVEN" 0 3 5, +C4<00000000000000000000000000000000>;
P_0x558fc950c810 .param/l "ODD" 0 3 5, +C4<00000000000000000000000000000001>;
v0x558fc94e6cf0_0 .net "clk", 0 0, v0x558fc9538250_0;  1 drivers
v0x558fc9537f80_0 .var "even_odd", 0 0;
v0x558fc9538040_0 .net "x", 0 0, v0x558fc9538320_0;  1 drivers
v0x558fc9538110_0 .var "z", 0 0;
E_0x558fc94e7480 .event posedge, v0x558fc94e6cf0_0;
    .scope S_0x558fc9521e90;
T_0 ;
    %wait E_0x558fc94e7480;
    %load/vec4 v0x558fc9537f80_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 1;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 1;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x558fc9538110_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x558fc9537f80_0, 0;
    %jmp T_0.3;
T_0.0 ;
    %load/vec4 v0x558fc9538040_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.4, 8;
    %pushi/vec4 1, 0, 2;
    %jmp/1 T_0.5, 8;
T_0.4 ; End of true expr.
    %pushi/vec4 0, 0, 2;
    %jmp/0 T_0.5, 8;
 ; End of false expr.
    %blend;
T_0.5;
    %pad/s 1;
    %assign/vec4 v0x558fc9538110_0, 0;
    %load/vec4 v0x558fc9538040_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.6, 8;
    %pushi/vec4 1, 0, 32;
    %jmp/1 T_0.7, 8;
T_0.6 ; End of true expr.
    %pushi/vec4 0, 0, 32;
    %jmp/0 T_0.7, 8;
 ; End of false expr.
    %blend;
T_0.7;
    %pad/s 1;
    %assign/vec4 v0x558fc9537f80_0, 0;
    %jmp T_0.3;
T_0.1 ;
    %load/vec4 v0x558fc9538040_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.8, 8;
    %pushi/vec4 0, 0, 2;
    %jmp/1 T_0.9, 8;
T_0.8 ; End of true expr.
    %pushi/vec4 1, 0, 2;
    %jmp/0 T_0.9, 8;
 ; End of false expr.
    %blend;
T_0.9;
    %pad/s 1;
    %assign/vec4 v0x558fc9538110_0, 0;
    %load/vec4 v0x558fc9538040_0;
    %flag_set/vec4 8;
    %jmp/0 T_0.10, 8;
    %pushi/vec4 0, 0, 32;
    %jmp/1 T_0.11, 8;
T_0.10 ; End of true expr.
    %pushi/vec4 1, 0, 32;
    %jmp/0 T_0.11, 8;
 ; End of false expr.
    %blend;
T_0.11;
    %pad/s 1;
    %assign/vec4 v0x558fc9537f80_0, 0;
    %jmp T_0.3;
T_0.3 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x558fc9521d00;
T_1 ;
    %vpi_call 2 8 "$dumpfile", "parity.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x558fc9521d00 {0 0 0};
    %vpi_call 2 9 "$monitor", $time, " x = %b, z = %b", v0x558fc9538320_0, v0x558fc95383f0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558fc9538250_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x558fc9521d00;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x558fc9538250_0;
    %inv;
    %store/vec4 v0x558fc9538250_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x558fc9521d00;
T_3 ;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558fc9538320_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558fc9538320_0, 0, 1;
