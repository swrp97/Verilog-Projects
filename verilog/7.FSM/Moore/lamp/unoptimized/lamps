#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55fd0bc83080 .scope module, "testlamp" "testlamp" 2 1;
 .timescale 0 0;
v0x55fd0bc95af0_0 .var "clk", 0 0;
v0x55fd0bc95bc0_0 .net "light", 0 3, v0x55fd0bc7ff60_0;  1 drivers
S_0x55fd0bc83210 .scope module, "LAMP" "cyclic_lamp" 2 4, 3 3 0, S_0x55fd0bc83080;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clock";
    .port_info 1 /OUTPUT 4 "light";
P_0x55fd0bc46cf0 .param/l "BLUE" 0 3 7, C4<0001>;
P_0x55fd0bc46d30 .param/l "GREEN" 0 3 7, C4<0100>;
P_0x55fd0bc46d70 .param/l "RED" 0 3 7, C4<1000>;
P_0x55fd0bc46db0 .param/l "S0" 0 3 6, +C4<00000000000000000000000000000000>;
P_0x55fd0bc46df0 .param/l "S1" 0 3 6, +C4<00000000000000000000000000000001>;
P_0x55fd0bc46e30 .param/l "S2" 0 3 6, +C4<00000000000000000000000000000010>;
P_0x55fd0bc46e70 .param/l "S3" 0 3 6, +C4<00000000000000000000000000000011>;
P_0x55fd0bc46eb0 .param/l "YELLOW" 0 3 7, C4<0010>;
v0x55fd0bc80260_0 .net "clock", 0 0, v0x55fd0bc95af0_0;  1 drivers
v0x55fd0bc7ff60_0 .var "light", 0 3;
v0x55fd0bc800e0_0 .var "state", 0 1;
E_0x55fd0bc806b0 .event posedge, v0x55fd0bc80260_0;
    .scope S_0x55fd0bc83210;
T_0 ;
    %wait E_0x55fd0bc806b0;
    %load/vec4 v0x55fd0bc800e0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 8, 0, 4;
    %assign/vec4 v0x55fd0bc7ff60_0, 0;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x55fd0bc800e0_0, 0;
    %jmp T_0.5;
T_0.0 ;
    %pushi/vec4 8, 0, 4;
    %assign/vec4 v0x55fd0bc7ff60_0, 0;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x55fd0bc800e0_0, 0;
    %jmp T_0.5;
T_0.1 ;
    %pushi/vec4 4, 0, 4;
    %assign/vec4 v0x55fd0bc7ff60_0, 0;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x55fd0bc800e0_0, 0;
    %jmp T_0.5;
T_0.2 ;
    %pushi/vec4 2, 0, 4;
    %assign/vec4 v0x55fd0bc7ff60_0, 0;
    %pushi/vec4 3, 0, 2;
    %assign/vec4 v0x55fd0bc800e0_0, 0;
    %jmp T_0.5;
T_0.3 ;
    %pushi/vec4 1, 0, 4;
    %assign/vec4 v0x55fd0bc7ff60_0, 0;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x55fd0bc800e0_0, 0;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55fd0bc83080;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x55fd0bc95af0_0;
    %inv;
    %store/vec4 v0x55fd0bc95af0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55fd0bc83080;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55fd0bc95af0_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 10 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55fd0bc83080;
T_3 ;
    %vpi_call 2 14 "$dumpfile", "lamp.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55fd0bc83080 {0 0 0};
    %vpi_call 2 14 "$monitor", $time, " RGYB: %b", v0x55fd0bc95bc0_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "lamptest.v";
    "lamp.v";
