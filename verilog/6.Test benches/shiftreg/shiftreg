 .timescale 0 0;
    .port_info 0 /INPUT 1 "clock";
    .port_info 1 /INPUT 1 "clear";
    .port_info 2 /INPUT 1 "A";
    .port_info 3 /OUTPUT 1 "E";
v0x55b6f206eba0_0 .net "A", 0 0, v0x55b6f20bbad0_0;  1 drivers
v0x55b6f20bb3e0_0 .var "B", 0 0;
v0x55b6f20bb4a0_0 .var "C", 0 0;
v0x55b6f20bb570_0 .var "D", 0 0;
v0x55b6f20bb630_0 .var "E", 0 0;
v0x55b6f20bb740_0 .net "clear", 0 0, v0x55b6f20bba00_0;  1 drivers
v0x55b6f20bb800_0 .net "clock", 0 0, v0x55b6f20bb940_0;  1 drivers
E_0x55b6f20a5b30/0 .event negedge, v0x55b6f20bb740_0;
E_0x55b6f20a5b30/1 .event posedge, v0x55b6f20bb800_0;
E_0x55b6f20a5b30 .event/or E_0x55b6f20a5b30/0, E_0x55b6f20a5b30/1;
    .scope S_0x55b6f20a7af0;
T_0 ;
    %wait E_0x55b6f20a5b30;
    %load/vec4 v0x55b6f20bb740_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55b6f20bb3e0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55b6f20bb4a0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55b6f20bb570_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55b6f20bb630_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55b6f20bb570_0;
    %assign/vec4 v0x55b6f20bb630_0, 0;
    %load/vec4 v0x55b6f20bb4a0_0;
    %assign/vec4 v0x55b6f20bb570_0, 0;
    %load/vec4 v0x55b6f20bb3e0_0;
    %assign/vec4 v0x55b6f20bb4a0_0, 0;
    %load/vec4 v0x55b6f206eba0_0;
    %assign/vec4 v0x55b6f20bb3e0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55b6f20944d0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b6f20bb940_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b6f20bba00_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b6f20bba00_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x55b6f20944d0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x55b6f20bb940_0;
    %inv;
    %store/vec4 v0x55b6f20bb940_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55b6f20944d0;
T_3 ;
    %delay 2, 0;
    %pushi/vec4 2, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b6f20bbad0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b6f20bbad0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b6f20bbad0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b6f20bbad0_0, 0, 1;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %end;
    .thread T_3;
    .scope S_0x55b6f20944d0;
T_4 ;
